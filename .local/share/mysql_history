drop table arau;
CREATE TABLE ARAU(
    Titolo VARCHAR(50) NOT NULL,
    FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,
    Autore VARCHAR(50) NOT NULL,
    FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION,
    PRIMARY KEY (Titolo, Autore)
);
CREATE TABLE ARAU(     Titolo VARCHAR(50) NOT NULL,     FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,     Autore VARCHAR(50) NOT NULL,     FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION,     PRIMARY KEY (Titolo, Autore) );
CREATE TABLE arau(
    Titolo VARCHAR(50) NOT NULL,
    FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,
    Autore VARCHAR(50) NOT NULL,
    FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION,
    PRIMARY KEY (Titolo, Autore)
);
CREATE TABLE arau(     Titolo VARCHAR(50) NOT NULL,     FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,     Autore VARCHAR(50) NOT NULL,     FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION,     PRIMARY KEY (Titolo, Autore) );
