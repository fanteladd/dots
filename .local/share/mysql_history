use articoli;
show tables;
create table RI(
Rivista VARCHAR(40) NOT NULL PRIMARY KEY,
Costo INT NOT NULL,
Editore VARCHAR(30) NOT NULL
);
create table RI( Rivista VARCHAR(40) NOT NULL PRIMARY KEY, Costo INT NOT NULL, Editore VARCHAR(30) NOT NULL );
CREATE TABLE QU(
    Qualif CHAR(1) NOT NULL PRIMARY KEY,
    Descrizione VARCHAR(30) NOT NULL
);
CREATE TABLE QU(     Qualif CHAR(1) NOT NULL PRIMARY KEY,     Descrizione VARCHAR(30) NOT NULL );
CREATE TABLE DIP(
    Dipnnum INT NOT NULL PRIMARY KEY,
    Nome VARCHAR(30) NOT NULL,
    Sede VARCHAR(30) NOT NULL
);
CREATE TABLE DIP(     Dipnnum INT NOT NULL PRIMARY KEY,     Nome VARCHAR(30) NOT NULL,     Sede VARCHAR(30) NOT NULL );
CREATE TABLE AR(
    Titolo VARCHAR(50) NOT NULL PRIMARY KEY,
    Rivista VARCHAR(40) NOT NULL,
    FOREIGN KEY (rivista) REFERENCES RI(rivista) ON UPDATE CASCADE ON DELETE NO ACTION,
    Data DATE NOT NULL
);
CREATE TABLE AR(     Titolo VARCHAR(50) NOT NULL PRIMARY KEY,     Rivista VARCHAR(40) NOT NULL,     FOREIGN KEY (rivista) REFERENCES RI(rivista) ON UPDATE CASCADE ON DELETE NO ACTION,     Data DATE NOT NULL );
CREATE TABLE AU(
    Autore VARCHAR(50) NOT NULL PRIMARY KEY,
    Qualif CHAR(1) NOT NULL,
    FOREIGN KEY (qualif) REFERENCES QU(qualif) ON UPDATE CASCADE ON DELETE NO ACTION,
    Data DATE NOT NULL,
    Dip INT NOT NULL,
    FOREIGN KEY (dip) REFERENCES DIP(dipnnum) ON UPDATE CASCADE ON DELETE NO ACTION,
    Sal INT NOT NULL
);
CREATE TABLE AU(     Autore VARCHAR(50) NOT NULL PRIMARY KEY,     Qualif CHAR(1) NOT NULL,     FOREIGN KEY (qualif) REFERENCES QU(qualif) ON UPDATE CASCADE ON DELETE NO ACTION,     Data DATE NOT NULL,     Dip INT NOT NULL,     FOREIGN KEY (dip) REFERENCES DIP(dipnnum) ON UPDATE CASCADE ON DELETE NO ACTION,     Sal INT NOT NULL );
CREATE TABLE ARAU(
    Titolo VARCHAR(50) NOT NULL PRIMARY KEY,
    Autore VARCHAR(50) NOT NULL PRIMARY KEY
);
CREATE TABLE ARAU(     Titolo VARCHAR(50) NOT NULL PRIMARY KEY,     Autore VARCHAR(50) NOT NULL PRIMARY KEY );
CREATE TABLE ARAU(
    Titolo VARCHAR(50) NOT NULL,
    Autore VARCHAR(50) NOT NULL,
    PRIMARY KEY (Titolo, Autore)
);
CREATE TABLE ARAU(     Titolo VARCHAR(50) NOT NULL,     Autore VARCHAR(50) NOT NULL,     PRIMARY KEY (Titolo, Autore) );
