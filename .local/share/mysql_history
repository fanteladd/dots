drop table arau;
CREATE TABLE arau(
    Titolo VARCHAR(50) NOT NULL,
    Autore VARCHAR(50) NOT NULL,
    PRIMARY KEY (Titolo, Autore)
    FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,
    FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION,
);
CREATE TABLE arau(     Titolo VARCHAR(50) NOT NULL,     Autore VARCHAR(50) NOT NULL,     PRIMARY KEY (Titolo, Autore)     FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,     FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION, );
CREATE TABLE arau(
    Titolo VARCHAR(50) NOT NULL,
    Autore VARCHAR(50) NOT NULL,
    PRIMARY KEY (Titolo, Autore)
    FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,
    FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION
);
CREATE TABLE arau(     Titolo VARCHAR(50) NOT NULL,     Autore VARCHAR(50) NOT NULL,     PRIMARY KEY (Titolo, Autore)     FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,     FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION );
CREATE TABLE arau(
    Titolo VARCHAR(50) NOT NULL,
    Autore VARCHAR(50) NOT NULL,
    PRIMARY KEY (Titolo, Autore),
    FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,
    FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION
);
CREATE TABLE arau(     Titolo VARCHAR(50) NOT NULL,     Autore VARCHAR(50) NOT NULL,     PRIMARY KEY (Titolo, Autore),     FOREIGN KEY (Titolo) REFERENCES AR(Titolo) ON UPDATE CASCADE ON DELETE NO ACTION,     FOREIGN KEY (Autore) REFERENCES AU(Autore) ON UPDATE CASCADE ON DELETE NO ACTION );
CREATE TABLE arau(     Titolo VARCHAR(50) NOT NULL,     Autore VARCHAR(50) NOT NULL,     PRIMARY KEY (Titolo, Autore));
alter table arau add foreign key (Autore) references au(Autore) on update cascade on delete no action;
alter table arau add foreign key (Titolo) references ar(Titolo) on update cascade on delete no action;
